<UserControl x:Class="Fishware_v5.Tabs.TabAdd"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Fishware_v5.Tabs"
             mc:Ignorable="d" 
             Height="400" Width="755" FontFamily="Gill Sans Nova">
    <Grid Background="#FF414B55">
        <Grid Height="345" VerticalAlignment="Bottom" Margin="32,0,32,45">
            <TextBox x:Name="txt_log" HorizontalAlignment="Left" Height="346" Width="458" TextWrapping="Wrap" VerticalAlignment="Top" Background="#FF232323" BorderBrush="#FF0BA7E2" Foreground="White" AcceptsReturn="True" IsReadOnly="True" VerticalContentAlignment="Bottom" Margin="118,-1,0,0"/>
        </Grid>
        <Grid Height="40" VerticalAlignment="Bottom">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Horizontal">
                <Button x:Name="FileFind" Content="Find File" Click="FileFind_Click"/>
            </StackPanel>
        </Grid>
    </Grid>
</UserControl>
